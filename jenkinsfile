pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'building the application'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Testing the application'
            }
        }
        
        stage('deploy) {
            steps {
                echo 'Deploying the application'
                echo 'Changes made for testing'
            }
        }
    }
}
